{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/chat",
      "dest": "api/chat.js"
    },
    {
      "src": "/api/health",
      "dest": "api/health.js"
    },
    {
      "src": "/api/version",
      "dest": "api/version.js"
    }
  ]
}